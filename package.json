  "devDependencies": {
    "browserify": "^13.0.1",
    "chokidar-cli": "^1.2.0",
    "shortid": "^2.2.6",
    "standard": "^7.1.2",
    "tap-notify": "^1.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "clone": "^1.0.2",
    "express": "^4.14.0",
